{
  "id": "builtin.language-basics-python",
  "name": "Language Basics Python",
  "description": "Provides syntax highlighting and bracket matching in Python files.",
  "languages": [
    {
      "id": "python",
      "extensions": [
        ".py"
      ],
      "fileNames": [
        "SCsub",
        "SConstruct"
      ],
      "configuration": "./languageConfiguration.json",
      "tokenize": "src/tokenizePython.js",
      "firstLine": "^#!\\s*/?.*\\bpython[0-9.-]*\\b"
    }
  ],
  "repository": "https://github.com/lvce-editor/language-basics-python"
}
